  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  font-size: 24rpx;
  text-align: center;
}

.partition-item {
  width: 20%;
  height: 140rpx;
  margin: 20rpx 10rpx;
}

.partition-icon {
  width: 80rpx;
  height: 80rpx;
}

.partition-type {
  display: block;
}