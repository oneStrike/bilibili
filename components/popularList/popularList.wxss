/* components/papularList/papularList.wxss */
.list-container {
	width: 100%;
	box-sizing: border-box;
	padding: 10rpx;
	/* margin-bottom: 20rpx; */
	border-bottom: 2rpx solid #eee;
	display: flex;
	font-size: 28rpx;
	flex-wrap: wrap;
	justify-content: center;
}

.cover-container {
	width: 260rpx;
	height: 180rpx;
	position: relative;
}

.v-cover {
	max-width: 100%;
	height: 100%;
	border-radius: 15rpx;
}

.v-duration {
	width: 80rpx;
	height: 40rpx;
	font-size: 24rpx;
	background-color: rgba(0, 0, 0, .5);
	color: #fff;
	text-align: center;
	line-height: 40rpx;
	position: absolute;
	bottom: 10rpx;
	right: 10rpx;
}

.v-brief {
	position: relative;
	flex: 1;
	font-size: 24rpx;
	margin-left: 10rpx;
	display: flex;
	flex-direction: column;
	justify-content: space-around;
}

.v-title {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	word-break: break-all;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	position: absolute;
	top: 0;
}

.v-up-name {
	background: url('data:image/jpg;base64,') no-repeat left;
	background-size: 30rpx;
	text-indent: 40rpx;
	position: relative;
}

.v-play-count {
	background: url('data:image/jpg;base64,') no-repeat left;
	background-size: 30rpx;
	text-indent: 40rpx;
	position: absolute;
	bottom: 0;
}

.v-pubdate {
	position: absolute;
	left: 50%;
	bottom: 0;
	transform: translateX(-50%)
}

.keyword {
	color: #fa7298;
	font-style: normal;
}